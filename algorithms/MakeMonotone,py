import sys
sys.path.insert(0,"..")
from datastructures.Edgebst import *
def makeMontone(dcel):
    def handleVertex(vertex):
        return








    vertices = dcel.vertices 
    vertices.sort(key=lambda coordinate: (coordinate.y, coordinate.x))
    eventList = EdgebstNode()
    while vertices.length != 0:
        vertex = vertices.pop 
        handleVertex(vertex)
    return dcel 


